{
  "app": {
    "global": {
      "defaults": {
        "client": "workingCopy"
      },
      "clients": {
        "workingCopy": {
          "scheme": "working-copy://",
          "xCallbackPath": "x-callback-url",
          "paramNames": {
            "remote": "repo",
            "branch": "branch",
            "sourceDir": "path",
            "key": "key"
          },
          "keyDir": "api/workingcopy/callbackUrl.txt",
          "actions": {
            "checkout": {
              "endpoint": "checkout",
              "specialParams": {
                "mode": "ensure"
              }
            },
            "commit": {
              "endpoint": "commit",
              "specialParams": {
                "limit": "999"
              }
            },
            "pull": {
              "endpoint": "pull"
            },
            "status": {
              "endpoint": "status"
            }
          }
        }
      }
    },
    "apps": {
      "Scriptable": {
        "repos": {
          "source": {
            "js": {
              "remote": "https://jimmy-zhening-luo@github.com/jimmy-zhening-luo/iOS-scriptable-ecosystem.git",
              "sourceDir": "src",
              "branches": {
                "mergeFromClone": "dev",
                "mergeToClone": "dev"
              }
            }
          }
        }
      }
    }
  }
}