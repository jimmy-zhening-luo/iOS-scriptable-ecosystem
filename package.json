{
  "name": "scriptable-wild",
  "version": "6.0.0-alpha.0",
  "description": "Building block classes and scripts to do complex automation on iOS.",
  "type": "module",
  "main": "",
  "scripts": {
    "lint": "eslint --cache --cache-strategy content -c eslint.config.js **",
    "lint:fix": "eslint --fix --cache --cache-strategy content -c eslint.config.js **",
    "clean:built": "shx rm -rf built",
    "clean:packed": "shx rm -rf packed",
    "clean": "npm run clean:built && npm run clean:packed",
    "prebuild": "npm run lint && npm run clean",
    "build": "tsc --build",
    "pack": "npm run build && shx mkdir packed && shx cp -r built/js/* packed",
    "upload": "npm run pack && ts-node --esm tools/upload.ts",
    "test": "No tests configured."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/scriptable-wild.git"
  },
  "author": "Jimmy Zhening Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/scriptable-wild/issues"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/scriptable-wild#readme",
  "engines": {
    "node": ">=18.18.2",
    "npm": ">=10.2.4"
  },
  "dependencies": {
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.3.3",
    "@types/node": "^18.18.9",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
