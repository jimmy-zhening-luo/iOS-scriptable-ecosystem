{
  "name": "scriptable-wild",
  "version": "6.0.0-alpha.0",
  "description": "Building block classes and scripts to do complex automation on iOS.",
  "type": "module",
  "main": "",
  "scripts": {
    "clean": "npm run clean:built && npm run clean:packed",
    "clean:built": "rimraf built",
    "clean:packed": "rimraf packed",
    "lint": "eslint --cache --cache-strategy content -c eslint.config.js src/**/*.ts",
    "prebuild": "npm run lint",
    "build": "tsc --build",
    "build:clean": "npm run clean:built && npm run build",
    "prepack": "npm run clean:packed && npm run build",
    "pack": "npm run pack:mkdir && npm run pack:cp",
    "pack:mkdir": "shx mkdir packed",
    "pack:cp": "shx cp -r built/js/* packed",
    "prepub": "npm run pack",
    "pub": "ts-node --esm tools/publish.ts",
    "pub:clean": "npm run clean:built && npm run pub",
    "test": "No tests configured."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jimmy-zhening-luo/scriptable-wild.git"
  },
  "author": "Jimmy Zhening Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimmy-zhening-luo/scriptable-wild/issues"
  },
  "homepage": "https://github.com/jimmy-zhening-luo/scriptable-wild#readme",
  "engines": {
    "node": ">=18.18.0",
    "npm": ">=10.1.0"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "rimraf": "^5.0.5"
  },
  "devDependencies": {
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "shx": "^0.3.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
